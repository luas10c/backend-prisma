{
  "name": "backend-prisma",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --env-file-if-exists .env.local dist/main.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsc-alias -f -p tsconfig.build.json",
    "lint": "eslint . --cache",
    "prisma:migrate": "prisma migrate",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "prisma": "^7.4.0",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "license": "UNLICENSED"
}
